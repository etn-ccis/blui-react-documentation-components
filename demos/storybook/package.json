{
  "name": "react-storybook",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prettier": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --write",
    "prettier:check": "prettier \"src/**/**.{ts,tsx,js,jsx,json,css,scss,html}\" --check",
    "precommit": "yarn prettier && yarn lint"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@brightlayer-ui/colors": "^3.1.1",
    "@brightlayer-ui/icons": "^1.11.0",
    "@brightlayer-ui/icons-mui": "^3.4.0",
    "@brightlayer-ui/prettier-config": "^1.0.3",
    "@brightlayer-ui/react-components": "^7.0.0",
    "@brightlayer-ui/react-doc-components": "^2.0.0",
    "@brightlayer-ui/react-themes": "^8.0.0",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@fontsource/roboto-mono": "^4.5.8",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.3",
    "@storybook/react": "^7.2.3",
    "@storybook/react-vite": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@types/prismjs": "^1.26.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "lodash.debounce": "^4.0.8",
    "prettier": "^3.0.3",
    "prismjs": "^1.29.0",
    "storybook": "^7.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "prettier": "@brightlayer-ui/prettier-config"
}
